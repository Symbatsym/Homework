1) def display_students(students):
    for student in students:
        print(f"Student: {student['name']}")
        for subject in student["subjects"]:
            score = student["scores"].get(subject)
            print(f"  {subject}: {score}")
        print() 


2) def get_average_score(name, students):
    for student in students:
        if student["name"] == name:
            scores = student["scores"].values()
            return sum(scores) / len(scores)
    return None

3) def find_top_student(students):
    top_name = None
    highest_avg = 0

    for student in students:
        scores = student["scores"].values()
        avg = sum(scores) / len(scores)

        if avg > highest_avg:
            highest_avg = avg
            top_name = student["name"]

    return top_name


4) def failed_students(students, passing_score=50):
    failed = []

    for student in students:
        for score in student["scores"].values():
            if score < passing_score:
                failed.append(student["name"])
                break

    return failed


5)def unique_subjects(data):
    unique = set()

    for student in data:
        for subject in student["subjects"]:
            unique.add(subject)

    return unique

